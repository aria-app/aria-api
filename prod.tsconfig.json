{
  "compilerOptions": {
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "lib": ["es6"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "outDir": "dist",
    "removeComments": true,
    "rootDir": "src",
    "sourceMap": false,
    "strict": true,
    "target": "ES2018"
  },
  "exclude": ["prisma/**/*"]
}
